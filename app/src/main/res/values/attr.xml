    <declare-styleable name="colorPickerView">
        <attr name="imageId" format="reference" />
        <attr name="thumbId" format="reference" />
    </declare-styleable>

    <declare-styleable name="WaterSurroundView">
        <attr name="waterHeight" format="dimension" />
